<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.bikeworld.bikeworld" >

    <uses-sdk android:minSdkVersion="8" />

    <uses-permission android:name="android.permission.INTERNET" />
    <!-- permiso de wifi state -->
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <android:uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <android:uses-permission android:name="android.permission.READ_PHONE_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Antiguos.MenuMainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Antiguos.LoginActivity"
            android:label="@string/title_activity_login"
            android:parentActivityName=".Antiguos.MenuMainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Antiguos.MenuMainActivity" />
        </activity>
        <activity
            android:name=".Antiguos.LoginActivity"
            android:label="@string/title_activity_login"
            android:parentActivityName=".Antiguos.MenuMainActivity"
            tools:ignore="DuplicateActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Antiguos.MenuMainActivity" />
        </activity>
        <activity
            android:name=".Antiguos.PantallaLogin"
            android:label="@string/title_activity_pantalla_login"
            android:parentActivityName=".Antiguos.MenuMainActivity"
            android:theme="@style/AppTheme.NoActionBar" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Antiguos.MenuMainActivity" />
        </activity>
        <activity
            android:name=".Antiguos.MenuPrincipalUsuario"
            android:label="@string/title_activity_menu_principal_usuario"
            android:parentActivityName=".Antiguos.MenuMainActivity"
            android:theme="@style/AppTheme.NoActionBar" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Antiguos.MenuMainActivity" />
        </activity>
        <activity
            android:name=".Antiguos.createUser"
            android:label="@string/title_activity_create_user"
            android:parentActivityName=".Antiguos.MenuMainActivity"
            android:theme="@style/AppTheme.NoActionBar" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Antiguos.MenuMainActivity" />
        </activity>
        <activity
            android:name=".ObjetosAntiguos.UserProfile"
            android:label="@string/title_activity_user_profile"
            android:parentActivityName=".Antiguos.MenuPrincipalUsuario"
            android:theme="@style/AppTheme.NoActionBar" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Antiguos.MenuPrincipalUsuario" />
        </activity>
        <activity
            android:name=".Antiguos.MenuPrincipalTipo2"
            android:label="@string/title_activity_menu_principal_tipo2"
            android:parentActivityName=".Antiguos.PantallaLogin"
            android:theme="@style/AppTheme.NoActionBar" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Antiguos.PantallaLogin" />
        </activity>
        <activity
            android:name=".Antiguos.MyVideos"
            android:configChanges="orientation"
            android:label="@string/title_activity_my_videos"
            android:parentActivityName=".ObjetosAntiguos.UserProfile"
            android:theme="@style/AppTheme.NoActionBar" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".ObjetosAntiguos.UserProfile" />
        </activity>
        <activity android:name="com.keyes.youtube.OpenYouTubePlayerActivity" />
        <activity
            android:name=".ObjetosAntiguos.VideoViewMostrar"
            android:label="@string/title_activity_video_view"
            android:parentActivityName=".Antiguos.MyVideos"
            android:theme="@style/AppTheme.NoActionBar" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Antiguos.MyVideos" />
        </activity>
        <activity
            android:name=".Player"
            android:label="@string/title_activity_player"
            android:parentActivityName=".Antiguos.MyVideos"
            android:theme="@style/AppTheme.NoActionBar" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Antiguos.MyVideos" />
        </activity>
        <activity
            android:name=".Antiguos.AllVideos"
            android:label="@string/title_activity_all_videos"
            android:parentActivityName=".Antiguos.MenuPrincipalTipo2"
            android:theme="@style/AppTheme.NoActionBar" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Antiguos.MenuPrincipalTipo2" />
        </activity>
        <activity
            android:name=".CodigoNuevo.NuevoMenuLateral"
            android:label="@string/title_activity_nuevo_menu_lateral"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name=".CodigoNuevo.NuevoMenuPrincipalColumnas"
            android:label="@string/title_activity_nuevo_menu_principal_columnas"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name=".CodigoNuevo.ContestarComentarios"
            android:label="@string/title_activity_contestar_comentarios"
            android:parentActivityName=".CodigoNuevo.NuevoMenuPrincipalColumnas"
            android:theme="@style/AppTheme.NoActionBar" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".CodigoNuevo.NuevoMenuPrincipalColumnas" />
        </activity>
    </application>

</manifest>
