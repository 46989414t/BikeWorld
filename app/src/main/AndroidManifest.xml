<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.bikeworld.bikeworld" >

    <uses-sdk android:minSdkVersion="8" />

    <uses-permission android:name="android.permission.INTERNET" />
    <!-- permiso de wifi state -->
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <android:uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <android:uses-permission android:name="android.permission.READ_PHONE_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MenuMainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LoginActivity"
            android:label="@string/title_activity_login"
            android:parentActivityName=".MenuMainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.bikeworld.bikeworld.MenuMainActivity" />
        </activity>
        <activity
            android:name=".LoginActivity"
            android:label="@string/title_activity_login"
            android:parentActivityName=".MenuMainActivity"
            tools:ignore="DuplicateActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.bikeworld.bikeworld.MenuMainActivity" />
        </activity>
        <activity
            android:name=".PantallaLogin"
            android:label="@string/title_activity_pantalla_login"
            android:parentActivityName=".MenuMainActivity"
            android:theme="@style/AppTheme.NoActionBar" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.bikeworld.bikeworld.MenuMainActivity" />
        </activity>
        <activity
            android:name=".MenuPrincipalUsuario"
            android:label="@string/title_activity_menu_principal_usuario"
            android:parentActivityName=".MenuMainActivity"
            android:theme="@style/AppTheme.NoActionBar" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.bikeworld.bikeworld.MenuMainActivity" />
        </activity>
        <activity
            android:name=".createUser"
            android:label="@string/title_activity_create_user"
            android:parentActivityName=".MenuMainActivity"
            android:theme="@style/AppTheme.NoActionBar" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.bikeworld.bikeworld.MenuMainActivity" />
        </activity>
        <activity
            android:name=".UserProfile"
            android:label="@string/title_activity_user_profile"
            android:parentActivityName=".MenuPrincipalUsuario"
            android:theme="@style/AppTheme.NoActionBar" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.bikeworld.bikeworld.MenuPrincipalUsuario" />
        </activity>
        <activity
            android:name=".MenuPrincipalTipo2"
            android:label="@string/title_activity_menu_principal_tipo2"
            android:parentActivityName=".PantallaLogin"
            android:theme="@style/AppTheme.NoActionBar" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.bikeworld.bikeworld.PantallaLogin" />
        </activity>
        <activity
            android:name=".MyVideos"
            android:configChanges="orientation"
            android:label="@string/title_activity_my_videos"
            android:parentActivityName=".UserProfile"
            android:theme="@style/AppTheme.NoActionBar" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.bikeworld.bikeworld.UserProfile" />
        </activity>
        <activity android:name="com.keyes.youtube.OpenYouTubePlayerActivity" />
        <activity
            android:name=".VideoViewMostrar"
            android:label="@string/title_activity_video_view"
            android:parentActivityName=".MyVideos"
            android:theme="@style/AppTheme.NoActionBar" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.bikeworld.bikeworld.MyVideos" />
        </activity>
        <activity
            android:name=".Player"
            android:label="@string/title_activity_player"
            android:parentActivityName=".MyVideos"
            android:theme="@style/AppTheme.NoActionBar" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.bikeworld.bikeworld.MyVideos" />
        </activity>
        <activity
            android:name=".AllVideos"
            android:label="@string/title_activity_all_videos"
            android:parentActivityName=".MenuPrincipalTipo2"
            android:theme="@style/AppTheme.NoActionBar" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.bikeworld.bikeworld.MenuPrincipalTipo2" />
        </activity>
    </application>

</manifest>
